{% extends 'base.html.twig' %}

{% block title %}Hello CommercialController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Hello {{ controller_name }}! ✅</h1>


    <form method="GET" action="">

        <div class="form-group">
            <h1>Recherche avancée</h1>
            <label for="InputNom" class="form-label mt-4">Nom</label>
            <input type="text" class="form-control" id="InputNom"  placeholder="Saisir un nom" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
        </div>
        <div class="form-group">
            <label for="exampleSelect1" class="form-label mt-4">Compétence</label>
            <select class="form-select" id="exampleSelect1">
                <option>PHP</option>
                <option>JAVA</option>
                <option>Comptable</option>
                <option>Symfony</option>
                <option>Directeur</option>
            </select>
        </div>
        <div class="form-group">
            <label for="exampleSelect1" class="form-label mt-4">Niveau de Compétence</label>
            <select class="form-select" id="exampleSelect1">
                <option>Expert</option>
                <option>Debutant</option>
                <option>Senior</option>
            </select>
        </div>
        <div><button type="submit" class="btn btn-primary">Rechercher</button></div>
    </form>

</div>
<form action="commercial/show.html.twig" method="POST">
    <div class="form-group">
        <label for="InputNom" class="form-label mt-4">Nom</label>
        <input type="text" class="form-control" id="InputNom"  placeholder="Entrer votre nom" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputPrenom" class="form-label mt-4">Prénom</label>
        <input type="text" class="form-control" id="InputPrenom" placeholder="Entrer votre prénom" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputTelephone" class="form-label mt-4">Téléphone</label>
        <input type="tel" class="form-control" id="InputTelephone" placeholder="Entrer votre téléphone" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputEmail" class="form-label mt-4">Email</label>
        <input type="email" class="form-control" id="InputEmail" placeholder="Entrer votre email" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputAdresse" class="form-label mt-4">Adresse</label>
        <input type="text" class="form-control" id="InputAdresse" placeholder="Entrer votre adresse" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputComplementAddresse" class="form-label mt-4">Complément d'addresse</label>
        <input type="text" class="form-control" id="InputComplementAddresse" placeholder="Entrer votre complément d'addresse" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputCodePostal" class="form-label mt-4">Code postal</label>
        <input type="number" class="form-control" id="InputCodePostal" placeholder="Entrer votre code postal" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputVille" class="form-label mt-4">Ville</label>
        <input type="text" class="form-control" id="InputVille" placeholder="Entrer votre ville" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputMdp" class="form-label mt-4">Mot de passe</label>
        <input type="password" class="form-control" id="InputMdp" placeholder="Entrer votre mot de passe" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div class="form-group">
        <label for="InputConfirmerMdp" class="form-label mt-4">Confirmer votre mot de passe</label>
        <input type="password" class="form-control" id="InputConfirmerMdp" placeholder="Confirmer votre mot de passe" style="background-image: url(&quot;data:image/png;base64,&quot;); background-repeat: no-repeat; background-attachment: scroll; background-size: 16px 18px; background-position: 98% 50%; cursor: auto;">
    </div>
    <div><button type="submit" class="btn btn-primary">Envoyer</button></div>
</form>

{% endblock %}